<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="EditTextWithTitle">
        <attr name="etTitle" format="string" />
        <attr name="etHint" format="string" />
        <attr name="etText" format="string" />
        <attr name="etInputType" format="enum">
            <enum name="text" value="0"/>
            <enum name="number" value="1"/>
            <enum name="decimal" value="2"/>
            <enum name="password" value="3"/>
            <enum name="email" value="4"/>
            <enum name="phone" value="5"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="RecyclerSpinner">
        <attr name="rsGravity" format="enum">
            <enum name="start" value="0"/>
            <enum name="center" value="1"/>
            <enum name="end" value="2"/>
        </attr>
    </declare-styleable>
</resources>